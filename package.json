{
  "name": "fishtrophy",
  "version": "1.0.0",
  "description": "Comprehensive platform for anglers in Romania",
  "private": true,
  "scripts": {
    "dev": "netlify dev",
    "dev:client": "cd client && npm run dev",
    "build": "npm run build:client",
    "build:client": "cd client && npm run build",
    "lint": "eslint client --ext .ts,.tsx --ignore-pattern '**/dist/**' --ignore-pattern '**/build/**' --ignore-pattern '**/node_modules/**'",
    "lint:fix": "npm run lint:fix:client",
    "lint:fix:client": "cd client && npm run lint:fix",
    "type-check": "npm run type-check:client",
    "type-check:client": "cd client && npm run type-check",
    "preview": "cd client && npm run preview",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:migrate": "drizzle-kit migrate"
  },
  "devDependencies": {
    "@netlify/functions": "^2.8.2",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "concurrently": "^8.2.2",
    "drizzle-kit": "^0.18.1",
    "eslint": "^8.57.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "netlify-cli": "^23.5.0",
    "prettier": "^3.3.3"
  },
  "engines": {
    "node": ">=20.0.0 <23.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.884.0",
    "@supabase/supabase-js": "^2.57.4"
  }
}
