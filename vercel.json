{
  "buildCommand": "pnpm build",
  "outputDirectory": "client/dist",
  "installCommand": "pnpm install",
  "framework": "vite",
  "functions": {
    "api/**/*.ts": {
      "runtime": "nodejs20.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ]
}
